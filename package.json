{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/automation-witness",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.0.96",
  "description": "XYO Archivist",
  "sideeffects": false,
  "engines": {
    "node": ">=16 <17"
  },
  "esm5": "dist/esm/index.js",
  "exports": {
    ".": {
      "esm5": "./dist/esm/index.js",
      "node": {
        "import": "./dist/cjs/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "start": "nodemon",
    "launch": "node --max-old-space-size=8192 --experimental-specifier-resolution=node ./dist/cjs/index.js",
    "test": "jest ./src",
    "test:hash": "jest ./src/hash",
    "test:domain": "jest ./src/domain",
    "test:schema": "jest ./src/schema"
  },
  "author": "Arie Trouw",
  "license": "LGPL-3.0",
  "dependencies": {
    "@ethersproject/address": "^5.7.0",
    "@ethersproject/contracts": "^5.7.0",
    "@ethersproject/networks": "^5.7.0",
    "@ethersproject/providers": "^5.7.0",
    "@ethersproject/solidity": "^5.7.0",
    "@xylabs/sdk-api-express-ecs": "^1.2.4",
    "@xylabs/sdk-js": "^2.6.7",
    "@xyo-network/coingecko-crypto-market-payload-plugin": "^2.31.0",
    "@xyo-network/crypto-asset-payload-plugin": "^2.31.0",
    "@xyo-network/etherchain-ethereum-gas-v1-payload-plugin": "^2.31.0",
    "@xyo-network/etherchain-ethereum-gas-v2-payload-plugin": "^2.31.0",
    "@xyo-network/etherscan-ethereum-gas-payload-plugin": "^2.31.0",
    "@xyo-network/sdk-xyo-client-js": "^2.31.0",
    "@xyo-network/uniswap-crypto-market-payload-plugin": "^2.31.0",
    "agenda": "^4.3.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "http-status-codes": "^2.2.0",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@babel/core": "^7.19.0",
    "@babel/preset-env": "^7.19.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/express": "4.17.13",
    "@types/jest": "^29.0.0",
    "@types/lodash": "^4.14.184",
    "@types/node": "^18.7.15",
    "@types/supertest": "^2.0.12",
    "@xylabs/eslint-config": "^2.7.9",
    "@xylabs/ts-scripts-yarn3": "^2.7.9",
    "@xylabs/tsconfig": "^2.7.9",
    "babel-jest": "^29.0.2",
    "jest": "^29.0.2",
    "jest-extended": "^3.1.0",
    "jest-mock-extended": "^2.0.7",
    "jest-sorted": "^1.0.14",
    "nodemon": "^2.0.19",
    "rimraf": "^3.0.2",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.8",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.2"
  },
  "packageManager": "yarn@3.2.3",
  "volta": {
    "node": "16.15.1",
    "yarn": "1.22.19"
  }
}
