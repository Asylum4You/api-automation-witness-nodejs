{
  "$schema": "http://json.schemastore.org/package.json",
  "name": "@xyo-network/automation-witness-runner",
  "publishConfig": {
    "access": "public"
  },
  "version": "2.0.96",
  "description": "XYO Automation Witness",
  "sideeffects": false,
  "engines": {
    "node": ">=16 <17"
  },
  "esm5": "dist/esm/index.js",
  "exports": {
    ".": {
      "esm5": "./dist/esm/index.js",
      "node": {
        "import": "./dist/cjs/index.js",
        "require": "./dist/cjs/index.js"
      },
      "default": "./dist/esm/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "scripts": {
    "start": "nodemon",
    "launch": "node --max-old-space-size=8192 --experimental-specifier-resolution=node ./dist/cjs/index.js",
    "test": "jest ./src",
    "test:hash": "jest ./src/hash",
    "test:domain": "jest ./src/domain",
    "test:schema": "jest ./src/schema"
  },
  "author": "Arie Trouw",
  "license": "LGPL-3.0",
  "dependencies": {
    "@xylabs/sdk-api-express-ecs": "^1.3.0",
    "@xyo-network/automation-witness-server": "^2.43.13",
    "dotenv": "^16.0.3",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.12",
    "@babel/preset-env": "^7.20.2",
    "@types/jest": "^29.2.6",
    "@types/supertest": "^2.0.12",
    "@xylabs/eslint-config": "^2.12.26",
    "@xylabs/ts-scripts-yarn3": "^2.12.26",
    "@xylabs/tsconfig": "^2.12.26",
    "babel-jest": "^29.4.0",
    "eslint": "^8.32.0",
    "jest": "^29.4.0",
    "jest-extended": "^3.2.3",
    "jest-mock-extended": "^3.0.1",
    "jest-sorted": "^1.0.14",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "packageManager": "yarn@3.2.3",
  "volta": {
    "node": "16.15.1",
    "yarn": "1.22.19"
  }
}
